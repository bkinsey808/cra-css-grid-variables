{
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Fira Code'",
  "editor.tokenColorCustomizations": {
    "comments": "#608b4e"
  },
  "explorer.openEditors.visible": 0,
  "jest.runAllTestsFirst": true,
  "jest.autoEnable": true,
  "jest.showCoverageOnLoad": true,
